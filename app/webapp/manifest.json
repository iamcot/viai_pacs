{
	"_version": "1.12.0",

	"sap.app": {
		"id": "vn.viaihealth.pacs_ui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"sourceTemplate": {
			"id": "generator-ui5-app",
			"version": "1.1.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "vn.viaihealth.pacs_ui.view.App",
			"type": "XML",
			"id": "app"
		},
		"flexEnabled": false,

		"dependencies": {
			"minUI5Version": "1.136.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt": {},
				"sap.f": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "vn.viaihealth.pacs_ui.i18n.i18n",
					"supportedLocales": ["en", "vi"],
					"fallbackLocale": ""
				}
			},
			"menu_header": {
				"preload": true,
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/menu_header.json"
			},
			"patients": {
				"preload": true,
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/patients.json"
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "vn.viaihealth.pacs_ui.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "visits",
					"name": "visits",
					"target": "visits"
				}
			
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				},
				"visits": {
					"viewId": "visits",
					"viewName": "Visits"
				}
			
			}
		}
	}
}
