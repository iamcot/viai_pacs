    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
>
    <core:Fragment
        fragmentName="vn.viaihealth.pacs_ui.view.layout.header"
        type="XML"
    />
    <FlexibleColumnLayout
        id="pacsFlexibleColumnLayout"
        stateChange=".onStateChange"
        backgroundDesign="Solid"
        layout="{/layout}"
    />
</mvc:View>
